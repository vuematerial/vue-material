{
  "name": "vue-material",
  "description": "Material Design for Vue.js",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "marcosmoura",
    "email": "marcosvmmoura@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuematerial/vue-material-experiments.git"
  },
  "bugs": {
    "url": "https://github.com/vuematerial/vue-material-experiments/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/vue-material.common.js",
  "module": "dist/vue-material.esm.js",
  "unpkg": "dist/vue-material.js",
  "scripts": {
    "dev": "babel-node build/local",
    "build": "rm -rf dist; babel-node build/lib",
    "lint": "eslint --ext .js,.vue docs src build test",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "postinstall": "babel-node build/git-hooks/install.js",
    "changelog": "babel-node build/generate-changelog.js",
    "release": "sh build/release.sh",
    "new-component": "babel-node build/new-component.js",
    "github-release": "github-release",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">= 7.0"
  },
  "dependencies": {
    "deepmerge": "^1.5.1",
    "popper.js": "^1.12.5",
    "vue": "^2.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "avoriaz": "^4.1.0",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.1.0",
    "clipboard": "^1.7.1",
    "commitizen": "^2.9.6",
    "concat": "^1.0.3",
    "connect-history-api-fallback": "^1.3.0",
    "conventional-changelog": "^1.1.5",
    "conventional-changelog-vue-material": "^0.0.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^3.13.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "github-release-cli": "^0.3.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.1.0",
    "jest-css-modules": "^1.1.0",
    "jest-vue": "^0.5.3",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^1.3.0",
    "postcss": "^6.0.11",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "sinon": "^3.2.1",
    "url-loader": "^0.5.9",
    "vue-i18n": "^7.2.0",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-style-loader": "^3.0.2",
    "vue-template-compiler": "^2.4.3",
    "vue-test-utils": "https://github.com/vuejs/vue-test-utils",
    "vuex": "^2.4.0",
    "vuex-router-sync": "^4.3.1",
    "webpack": "2.7.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "<rootDir>/node_modules/vue/dist/vue.common.js",
      "^vue-material(.*)$": "<rootDir>/src$1",
      "^test(.*)$": "<rootDir>/test$1",
      "^src(.*)$": "<rootDir>/src$1",
      "^theme(.*)$": "<rootDir>/src/theme$1",
      "^base(.*)$": "<rootDir>/src/base$1",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^components(.*)$": "<rootDir>/src/components$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest",
      ".*\\.(vue)$": "jest-vue",
      ".*": "jest-css-modules"
    },
    "mapCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/**/index.js",
      "!src/**/*.test.js"
    ],
    "coverageReporters": [
      "lcov",
      "json",
      "html"
    ]
  },
  "browserslist": [
    "> 1%"
  ]
}
